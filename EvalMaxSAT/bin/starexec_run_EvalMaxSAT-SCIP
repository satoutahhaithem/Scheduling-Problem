#!/bin/bash
wl=3600
mem=32768

echo "c [MSE2023] -- begin -- [MSE2023]"
xz -cdk $1 > maxsat.wcnf
./run --timestamp -d 10 -o output.out -v output.var -w output.wat -C $wl -W $wl -M $mem ./EvalMaxSAT --timeUB 0 --minRefTime 5 maxsat.wcnf
cat output.out
cat output.var
rm -f maxsat.wcnf
rm -f output.out
rm -f output.var
rm -f output.wat
echo "c [MSE2023] -- end -- [MSE2023]"
